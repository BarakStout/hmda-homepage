{
  "name": "hmda-homepage",
  "version": "0.0.0",
  "description": "Homepage for routing to HMDA tools",
  "main": "index.js",
  "engines": {
    "node": ">=8.x"
  },
  "scripts": {
    "test": "jest",
    "clearDist": "rimraf ./dist && mkdirp ./dist ./dist/js ./dist/css ./dist/fonts ./dist/img",
    "clearBackup": "rimraf ./dist/js/app.min.js.bak",
    "env": "./env.sh",
    "js": "(export NODE_ENV=build; webpack --progress)",
    "js:dev": "export NODE_ENV=build && webpack --config webpack.dev.js --progress",
    "sass": "node-sass-chokidar --quiet --output-style=compressed --source-map=./dist/css/ ./src/app.scss ./dist/css/app.min.css && yarn run sass:prefix",
    "fonts": "cp node_modules/uswds/src/fonts/sourcesanspro-* ./dist/fonts",
    "sass:prefix": "postcss -u autoprefixer -r ./dist/css/*.css",
    "html": "html-minifier --collapse-whitespace ./src/index.html -o ./dist/index.html",
    "img": "cp -r ./node_modules/uswds/dist/img/* ./dist/img && cp ./node_modules/hmda-ui/img/* ./dist/img",
    "postinstall": "yarn run clearDist && yarn run build",
    "watch:js": "yarn run js:dev -- --watch",
    "watch:sass": "node-sass-chokidar --watch --quiet --output-style=compressed --source-map=./dist/css/ ./src/sass/app.scss ./dist/css/app.min.css",
    "watch:tests": "jest --watchAll",
    "watch": "yarn run watch:js & yarn run watch:sass",
    "build": "yarn run js && yarn run sass && yarn run fonts && yarn run html && yarn run img",
    "pretty": "yarn run prettier --config ./.prettierrc --write './{src,__tests__}/**/*.{js*,scss}'",
    "lint": "eslint --ext .js,.jsx ./src"
  },
  "keywords": [
    "HMDA"
  ],
  "author": "Consumer Financial Protection Bureau",
  "license": "CC0-1.0",
  "dependencies": {
    "hmda-ui": "0.0.4",
    "prop-types": "15.6.0",
    "react": "16.3.2",
    "react-dom": "16.3.2",
    "uswds": "1.4.6"
  },
  "devDependencies": {
    "@babel/core": "7.0.0-beta.34",
    "@babel/plugin-proposal-object-rest-spread": "7.0.0-beta.34",
    "@babel/polyfill": "7.0.0-beta.34",
    "@babel/preset-env": "7.0.0-beta.34",
    "@babel/preset-react": "7.0.0-beta.34",
    "autoprefixer": "8.0.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "8.2.3",
    "babel-jest": "21.2.0",
    "babel-loader": "8.0.0-beta.0",
    "eslint": "4.19.1",
    "eslint-plugin-babel": "5.1.0",
    "eslint-plugin-react-app": "3.0.0",
    "html-minifier": "3.5.9",
    "jest-cli": "22.3.0",
    "mkdirp": "0.5.1",
    "node-sass-chokidar": "0.0.3",
    "postcss-cli": "5.0.0",
    "prettier": "1.12.1",
    "rimraf": "2.6.2",
    "webpack": "3.11.0",
    "webpack-merge": "4.1.1",
    "webpack-shell-plugin": "0.5.0",
    "why-did-you-update": "0.1.0"
  },
  "jest": {
    "transform": {
      ".+\\.jsx?$": "babel-jest"
    },
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/lodash",
      "<rootDir>/node_modules/react-dom",
      "<rootDir>/node_modules/react-addons-test-utils",
      "<rootDir>/node_modules/fbjs"
    ]
  }
}
